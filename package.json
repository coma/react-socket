{
    "name"           : "react-socket",
    "description"    : "A neat way of handling views on other components",
    "homepage"       : "https://github.com/coma/react-socket",
    "repository"     : "https://github.com/coma/react-socket",
    "main"           : "./src/index.js",
    "version"        : "0.1.4",
    "license"        : "MIT",
    "private"        : false,
    "authors"        : [
        "Eduardo Garc√≠a Sanz <eduardo@comakai.com>"
    ],
    "keywords"       : [
        "react",
        "socket.io",
        "react-component"
    ],
    "dependencies"   : {
        "socket.io-client": "^1.4"
    },
    "peerDependencies"   : {
        "react"           : "^0.13 || ^0.14 || ^15.0.0"
    },
    "devDependencies": {
        "proxyquire": "1.5",
        "jsdom"     : "5.4",
        "mocha"     : "2.2",
        "should"    : "7.0",
        "eslint"    : "0.24"
    },
    "scripts"        : {
        "lint": "eslint src",
        "test": "npm run lint && mocha"
    },
    "eslintConfig"   : {
        "env"  : {
            "node": true
        },
        "rules": {
            "strict"                     : 0,
            "new-cap"                    : 0,
            "eqeqeq"                     : 2,
            "curly"                      : 2,
            "quotes"                     : [
                1,
                "single"
            ],
            "no-unreachable"             : 2,
            "space-before-blocks"        : 2,
            "space-before-function-paren": 2,
            "no-multi-spaces"            : 0,
            "key-spacing"                : 0,
            "no-mixed-requires"          : 0
        }
    }
}
